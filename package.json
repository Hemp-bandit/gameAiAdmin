{
  "name": "gameAiAdmin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "release": "npm-run-all build release:cp release:install release:tgz release:dep",
    "release:cp": "shx cp package.json .env  package-lock.lock docker-compose.yml  $npm_package_name/",
    "release:install": "cd $npm_package_name && npm install --production",
    "release:tgz": "tar -zcvf $npm_package_name.tgz $npm_package_name ",
    "release:dep": "scp $npm_package_name.tgz huawei:/root/project"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "^1.0.2-beta.55",
    "ip-input-vue": "^1.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "normalizecss": "^3.0.0",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.171",
    "@types/moment": "^2.13.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-plugin-import": "^1.13.3",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "typescript": "~3.9.3"
  },
  "author": "wyswill"
}